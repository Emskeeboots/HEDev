#include "com_source"
///////////////////////////////////////////////////////////////////////////////
/////////////////////Mad Rabbit's Player Chat Commands/////////////////////////
///////////////////////////////////////////////////////////////////////////////

///////////////////////////////Declarations////////////////////////////////////

///////////////////////////////Definition//////////////////////////////////////

////////////////////////////////Main Code//////////////////////////////////////

void main()
{
    object oPC = OBJECT_SELF;
    string sMessage = GetPCChatMessage();
    string sSecondaryCommand = GetSubString(sMessage, 5, PC_COMMAND_MAX_COMMAND_LENGTH);

    if (sSecondaryCommand == "front")
        ExecuteScript("com_hk_1", oPC);
    else if (sSecondaryCommand == "behind")
        ExecuteScript("com_hk_2", oPC);
    else if (sSecondaryCommand == "back up")
        ExecuteScript("com_hk_3", oPC);
}
